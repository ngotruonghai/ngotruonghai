@model IEnumerable<Nganhtinhoc.Models.Sinhvien>

@{
    ViewBag.Title = "get_sinhvien";
    Layout = "~/Views/Shared/sinhvien_layout.cshtml";
}
<div class="row">
    <div class="col-lg-12 col-xs-12 col-md-12">
        @foreach (var i in Model)
        {
            <h1 style="text-align:center;color:#0061AD"><strong>@i.tensinhvien &ensp; mssv:@i.masinhvien</strong></h1>
        }
    </div>
</div>
@foreach (var i in Model)
{
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="col-lg-3 col-xs-6 col-md-6"><a href="/thoi_khoa_bieu/@i.manganh"><img src="~/Content/ing_ero/sinhvien/tkb.jfif" class="img-responsive" style="background-color: #ccc;-webkit-box-shadow: 0 8px 6px -6px black; -moz-box-shadow: 0 8px 6px -6px black;box-shadow: 0 8px 6px -6px black;width:200px;height:200px" /></a><h4 style="margin-left:35px"><strong>Thời khóa biểu</strong></h4></div>
            <div class="col-lg-3 col-xs-6 col-md-6"><a href="/diem/@i.masinhvien"><img src="~/Content/ing_ero/sinhvien/sv_hoctap.jpg" class="img-responsive" style="background-color: #ccc;-webkit-box-shadow: 0 8px 6px -6px black; -moz-box-shadow: 0 8px 6px -6px black;box-shadow: 0 8px 6px -6px black;width:200px;height:200px" /></a><h4 style="margin-left:35px"><strong>Kết quả học tập</strong></h4></div>
        </div>
    </div>
}
